
@model WebAuthent.Pages.Atencion.CreateModel

<h1>Dar de alta nuevo municipio</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="CatMunicipio.Nombre" class="control-label"></label>
                <input asp-for="CatMunicipio.Nombre" class="form-control" />
                <span asp-validation-for="CatMunicipio.Nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CatMunicipio.Descripcion" class="control-label"></label>
                <input asp-for="CatMunicipio.Descripcion" class="form-control" />
                <span asp-validation-for="CatMunicipio.Descripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
